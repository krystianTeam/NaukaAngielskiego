var Pytanie= class Pytanie{
    var A;
    var B;
    construct(z , y){
        A = z;
        B = y;
    }
    
    
    
    drukuj() {
        document.getElementById("odpA").innerHTML = A;
        document.getElementById("odpB").innerHTML = B;
    }
    
}

var p = new Pytanie("dwadwad" , "fefesf");
p.drukuj();
